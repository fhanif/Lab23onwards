<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.gc.models.UserinfoEntity" table="userinfo" schema="coffeeshop">
        <id name="userId">
            <column name="UserID" sql-type="int(10) unsigned"/>
        </id>
        <property name="firstName">
            <column name="FirstName" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="lastName">
            <column name="LastName" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="phoneNumber">
            <column name="phoneNumber" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="userName">
            <column name="userName" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>